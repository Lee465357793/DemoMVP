<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.weiyi.mvpdemo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme">
        <activity
            android:name=".v.activity.MainActivity"
            android:configChanges="screenSize|smallestScreenSize|screenLayout|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".v.activity.TabLayoutActivity">
        </activity>
        <activity android:name=".v.activity.CoordinatorActivity">
        </activity>
        <activity android:name=".v.activity.ToolbarForActionBarActivity">
        </activity>
        <activity android:name=".v.activity.AnimActivity">
        </activity>

        <service
            android:name=".common.service.AidlService"
            android:enabled="true"
            android:exported="true">
        </service>

        <activity android:name=".v.activity.ConstraintActivity">
        </activity>
        <activity android:name=".v.activity.ImgViewPagerActivity">
        </activity>
        <activity android:name=".v.activity.TransitionDemoActivity">
        </activity>
        <activity android:name=".v.activity.MenuActivity">
        </activity>
    </application>

</manifest>